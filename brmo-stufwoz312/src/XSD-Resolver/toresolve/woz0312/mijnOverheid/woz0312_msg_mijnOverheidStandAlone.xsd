	<!--SimpleTypes-->
	<simpleType name="DiepUrl">
		<restriction base="string">
			<maxLength value="512"/>
		</restriction>
	</simpleType>
	<simpleType name="Maateenheid11">
		<restriction base="nonNegativeInteger">
			<totalDigits value="11"/>
		</restriction>
	</simpleType>
	<simpleType name="Maateenheid11-neg">
